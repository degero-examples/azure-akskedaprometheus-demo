{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "agentPoolMaxCount": {
      "value": 2
    },
    "grafanaUsers": {
      "value": []
    },
    "vmSize": {
      "value": "Standard_B2s"
    },
    "nodePools": {
      "value": [
        {
          "name": "appone",
          "sku": "Standard_B2s",
          "count": 0,
          "minCount": 0,
          "maxCount": 2,
          "nodeLabels": {
            "app": "app-one"
          }
        },
        {
          "name": "apptwo",
          "sku": "Standard_B2s",
          "count": 0,
          "minCount": 0,
          "maxCount": 2,
          "nodeLabels": {
            "app": "app-two"
          }
        }
      ]
    }
  }
}